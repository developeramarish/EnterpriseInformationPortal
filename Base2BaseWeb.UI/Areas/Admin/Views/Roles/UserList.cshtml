@model IEnumerable<AppUser>

<h4>Список пользователей</h4>
<div class="container-fluid">
    <div class="row">
        <div class="col-md-9">
            <table class="table">
                <tr>
                    <th>Имя</th>
                    <th>Эл. почта</th>
                    <th></th>
                </tr>
                @foreach (var user in Model)
                {
                    <tr>
                        <td>@user.UserName</td>
                        <td>@user.Email</td>
                        <td>
                            <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Права доступа</a>
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>


