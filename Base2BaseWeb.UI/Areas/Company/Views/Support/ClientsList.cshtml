@model SupportIndexViewModel

<table id="ticketsTable" class="table table-hover table-responsive table-striped table-bordered clients-thead-grey">
    <thead class="clients-thead-grey">
        <tr>
            <th class="col-md-2 text-center">Дата</th>
            <th class="col-md-3 text-center">Клиент</th>
            <th class="col-md-2 text-center">Тема заявки</th>
            <th class="col-md-1 text-center">Создание</th>
            <th class="col-md-1 text-center">Окончание</th>
            <th class="col-md-1 text-center">Время</th>
            <th class="col-md-2 text-center">Состояние</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var ticket in Model.Tickets)
        {
        <tr>
            <td class="text-center">@ticket.DateCreated.GetValueOrDefault().ToShortDateString() </td>
            <td><a href="#">@ticket.ClientName</a></td>
            <td class="text-center">@ticket.TicketSubjectName</td>
            <td class="text-center">@ticket.DateCreated.GetValueOrDefault().ToShortTimeString() </td>
            <td class="text-center">@ticket.DateFinished.GetValueOrDefault().ToShortTimeString() </td>
            <td class="text-center">@ticket.TimeElapsed</td>
            <td class="text-center">@ticket.TicketStatusName</td>
        </tr>
        }
    </tbody>
</table>